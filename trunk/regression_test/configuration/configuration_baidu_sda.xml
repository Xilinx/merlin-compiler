<configs> 

  <config>
      <name>full</name>
      <case_type>org</case_type>
      <case>all</case>
      <flow>bit</flow>
      <platform_name>baidu_kintex7</platform_name>
      <part_name>xc7k480tiffg1156-2L</part_name>
      <arch_name>kintex7</arch_name>
  </config>  

  <config>
      <name>pipe_off</name>
      <case_type>org</case_type>
      <case>all</case>
      <flow>bit</flow>
      <platform_name>baidu_kintex7</platform_name>
      <part_name>xc7k480tiffg1156-2L</part_name>
      <arch_name>kintex7</arch_name>
      <pipeline_parallel> off </pipeline_parallel>                 
  </config>  

  <config>
      <name>burst_off</name>
      <case_type>org</case_type>
      <case>all</case>
      <flow>bit</flow>
      <platform_name>baidu_kintex7</platform_name>
      <part_name>xc7k480tiffg1156-2L</part_name>
      <arch_name>kintex7</arch_name>
      <memory_burst     > off </memory_burst>                      
  </config>  

  <config>
      <name>bitwidth_off</name>
      <case_type>org</case_type>
      <case>all</case>
      <flow>bit</flow>
      <platform_name>baidu_kintex7</platform_name>
      <part_name>xc7k480tiffg1156-2L</part_name> 
      <arch_name>kintex7</arch_name>
      <bitwidth_opt     > off </bitwidth_opt>
  </config>  

  <config>
      <name>reduction_off</name>
      <case_type>org</case_type>
      <case>all</case>
      <flow>bit</flow>
      <platform_name>baidu_kintex7</platform_name>
      <part_name>xc7k480tiffg1156-2L</part_name>
      <arch_name>kintex7</arch_name>
      <reduction_opt    > off </reduction_opt>
  </config>  

  <config>
      <name>pcie_off</name>
      <case_type>org</case_type>
      <case>all</case>
      <flow>bit</flow>
      <platform_name>baidu_kintex7</platform_name>
      <part_name>xc7k480tiffg1156-2L</part_name>
      <arch_name>kintex7</arch_name>
      <pcie_transfer_opt> off </pcie_transfer_opt>                 
  </config>  

  <config>
      <name>coarse_off</name>
      <case_type>org</case_type>
      <case>all</case>
      <flow>bit</flow>
      <platform_name>baidu_kintex7</platform_name>
      <part_name>xc7k480tiffg1156-2L</part_name>
      <arch_name>kintex7</arch_name>
      <coarse_grained_pipeline> off </coarse_grained_pipeline>     
  </config>  

  <config>
      <name>naive_on</name>
      <case_type>org</case_type>
      <case>all</case>
      <flow>bit</flow>
      <platform_name>baidu_kintex7</platform_name>
      <part_name>xc7k480tiffg1156-2L</part_name>
      <arch_name>kintex7</arch_name>
      <naive_hls> on </naive_hls>
  </config>  

</configs>
